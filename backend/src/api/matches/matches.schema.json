{
  "$schema": "http://json-schema.org/draft-07/schema",

  "type": "object",
  "title": "Match",
  "description": "Match",
  "required": [
    "player1",
    "player2",
    "date_played",
    "win",
    "player1_score",
    "player2_score",
    "player1_total_points",
    "player2_total_points",
    "set_score"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "Match ID"
    },
    "player1": {
      "$id": "#/properties/player1",
      "type": "string",
      "title": "Username of player 1"
    },
    "player2": {
      "$id": "#/properties/player2",
      "type": "string",
      "title": "Username of player 2"
    },
    "date_played": {
      "$id": "#/properties/date_played",
      "type": "string",
      "title": "Players gender"
    },
    "win": {
      "$id": "#/properties/win",
      "type": "string",
      "title": "Username of winner"
    },
    "defeat": {
      "$id": "#/properties/defeat",
      "type": "string",
      "title": "Username of loser"
    },
    "player1_score": {
      "$id": "#/properties/player1_score",
      "type": "integer",
      "title": "Match score"
    },
    "player2_score": {
      "$id": "#/properties/player2_score",
      "type": "integer",
      "title": "Match score"
    },
    "player1_total_points": {
      "$id": "#/properties/player1_total_points",
      "type": "integer",
      "title": "Total points played"
    },
    "player2_total_points": {
      "$id": "#/properties/player2_total_points",
      "type": "integer",
      "title": "Total points played"
    },
    "set_score": {
      "$id": "#/properties/score",
      "type": "array",
      "title": "Match score"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "Creation date of match"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "Last update of match"
    }
  }
}
